[
    {
        "name": "bfp-scraper",
        "image": "129112697295.dkr.ecr.us-east-1.amazonaws.com/bfp:latest",
        "portMappings": [
            {
                "containerPort": 80,
                "protocol": "tcp"
            },
            {
                "containerPort": 443,
                "protocol": "tcp"
            }
        ],
        "essential": true,
        "environmentFiles": [
            {
                "value": "arn:aws:s3:::bfp-fargate/bfp-prod.env",
                "type": "s3"
            }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
               "awslogs-group" : "/fargate/bfp-prod-scraper-log",
               "awslogs-region": "us-east-1",
               "awslogs-stream-prefix": "fargate"
            }
         }
    }
]
